#include <stdio.h>

#define m 20 
int main() {
    int HT[m];
    int N, key, index, i;

   
    for (i = 0; i < m; i++) {
        HT[i] = -1;
    }

    printf("Enter number of employee records: ");
    scanf("%d", &N);

    for (i = 0; i < N; i++) {
        printf("Enter 4-digit key for employee %d: ", i + 1);
        scanf("%d", &key);

        index = key % m; 
     
        while (HT[index] != -1) {
            index = (index + 1) % m;
        }

        HT[index] = key;
    }

    printf("\nHash Table:\n");
    printf("Address\tKey\n");
    for (i = 0; i < m; i++) {
        if (HT[i] != -1)
            printf("%d\t%d\n", i, HT[i]);
        else
            printf("%d\t---\n", i);
    }

    return 0;
}
